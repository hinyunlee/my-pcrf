  Routing-Rule-Remove ::= < AVP Header: 1075 >
                         *[ Routing-Rule-Identifier ]
                         *[ AVP ]

  Routing-Rule-Definition ::= < AVP Header: 1076 >
                              { Routing-Rule-Identifier }
